{
  "name": "xt-api-wrapper",
  "version": "1.0.1",
  "description": "TypeScript wrapper for XT-API social media downloader - Extract media from Instagram, Facebook, Twitter, TikTok, YouTube, and more",
  "author": "XT-API Team",
  "license": "GPL-3.0",
  "keywords": [
    "social-media",
    "downloader",
    "api-wrapper",
    "instagram",
    "facebook",
    "twitter",
    "tiktok",
    "youtube",
    "weibo",
    "reddit",
    "bilibili",
    "soundcloud",
    "pixiv",
    "video-downloader",
    "media-extractor",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/risuncode/xt-api-wrapper"
  },
  "bugs": {
    "url": "https://github.com/risuncode/xt-api-wrapper/issues"
  },
  "homepage": "https://github.com/risuncode/xt-api-wrapper#readme",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.mts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "build": "tsup",
    "build:types": "tsc --emitDeclarationOnly",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "tsx": "^4.21.0",
    "typescript": "^5.3.3"
  }
}
